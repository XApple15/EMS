name: dockercompose1163020246386950322
services:
  auth-service:
    build:
      context: C:\Users\George\source\repos\EMS-app
      dockerfile: auth-service/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: authservice
    labels:
      traefik.enable: "true"
      traefik.http.routers.auth.rule: PathPrefix(`/auth`)
      traefik.http.services.auth.loadbalancer.server.port: "8080"
    networks:
      ems-net: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\George\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\George\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  sqlserver:
    container_name: sqlserver
    environment:
      ACCEPT_EULA: "Y"
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      ems-net: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
  traefik:
    command:
      - --api.insecure=true
      - --providers.docker=true
      - --entrypoints.web.address=:80
    container_name: traefik
    image: traefik:v3.1
    networks:
      ems-net: null
    ports:
      - mode: ingress
        target: 80
        published: "80"
        protocol: tcp
      - mode: ingress
        target: 8080
        published: "8080"
        protocol: tcp
    volumes:
      - type: bind
        source: /var/run/docker.sock
        target: /var/run/docker.sock
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\George\source\repos\EMS-app\traefik.yml
        target: /etc/traefik/traefik.yml
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\George\source\repos\EMS-app\dynamic_conf.yml
        target: /etc/traefik/dynamic_conf.yml
        bind:
          create_host_path: true
networks:
  ems-net:
    name: dockercompose1163020246386950322_ems-net
    driver: bridge